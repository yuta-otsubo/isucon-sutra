{
  "name": "frontend",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "API_BASE_URL=/api remix vite:build",
    "dev": "API_BASE_URL=/api remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "preview": "vite preview",
    "start": "pnpm run build && API_BASE_URL=/api pnpm run preview",
    "typecheck": "tsc",
    "fmtcheck": "prettier ./*.{cjs,ts,js} ./app --check",
    "fmt": "prettier ./*.{cjs,ts,js} ./app --write",
    "codegen": "rm -rf ./app/api && npx openapi-codegen gen isucon"
  },
  "dependencies": {
    "@remix-run/node": "^2.16.8",
    "@remix-run/react": "^2.16.8",
    "@tanstack/react-query": "^5.81.2",
    "canvas-confetti": "^1.9.3",
    "event-source-polyfill": "^1.0.31",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@openapi-codegen/cli": "2.0.2",
    "@openapi-codegen/typescript": "~8.0.2",
    "@remix-run/dev": "^2.16.8",
    "@types/canvas-confetti": "^1.9.0",
    "@types/event-source-polyfill": "^1.0.5",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "fdir": "^6.4.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.26.1+sha256.e8e6e499128f6804f5c92223cd1f2e3f62557102c0c6e254cc99bb2aa229bbf9"
}
